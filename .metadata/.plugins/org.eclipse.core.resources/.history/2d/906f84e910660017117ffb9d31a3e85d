#ifndef DEFINITIONS_H_
#define DEFINITIONS_H_

#define uint32 unsigned int
#define uint16 unsigned short
#define uint8 unsigned char
#define int32 int
#define int16 short
#define int8 char

#define AXIS_X 0
#define AXIS_Y 1
#define AXIS_Z 2
#define AXIS_SIZE 3

#define DATA_SIZE 128
#define FFT_SIZE 64

#endif /* DEFINITIONS_H_ */
