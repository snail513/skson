// system error code definition
#define ERR_DEV_RANGE_OVER		4100
#define ERR_DEV_INVALID_VALUE	4101
#define ERR_DIVID_BY_ZERO		4102
#define ERR_INVALID_FLOAT		4140
#define ERR_FLOAT_RANGE_OVER	4141
#define ERR_STRING_LENGTH_OVER	4151
#define ERR_IO_MODULE_COMM		4160

// for date_time register access
// ex) PLC_SD_REG_DATE_TIME+REG_YEAR
enum
{
	REG_YEAR=0,
	REG_MON,
	REG_DAY,
	REG_HOUR,
	REG_MIN,
	REG_SEC,
	REG_WDAY,
};

