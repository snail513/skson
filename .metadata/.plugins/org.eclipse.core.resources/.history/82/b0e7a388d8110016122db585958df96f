#ifndef ADDTEST_H_
#include "AddTest.h"

typedef enum
{
	none = 0,
	srcbit,
	srcWord,
	srcDWord,
	srcFloat,
	srcDouble,
	dstbit,
	dstWord,
	dstDWord,
	dstFloat,
	dstDouble,
	cnt
}TestType;

typedef struct
{
	uint16 operatorML;
	uint8 operandCount;
	TestType operands[5];
}OperandStruct;

static OperandStruct currStruct;

uint8 setOperandStruct(uint16 operatorML, uint8 operandCount, TestType t[])
{
	if(operandCount>5)
		return FALSE;
	currStruct.operandCount = operandCount;
	memcpy(currStruct.operands, t, sizeof*(TestType)*operandCount);
	return TRUE;
}



uint8 test_main(int argc, char* argv[])
{

}
#endif


